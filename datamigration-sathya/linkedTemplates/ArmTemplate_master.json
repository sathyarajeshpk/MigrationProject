{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "datamigration-sathya"
		},
		"MySqlonprem_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'MySqlonprem'"
		},
		"MySqlonprem_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "localhost"
		},
		"MySqlonprem_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().dbName}"
		},
		"MySqlonprem_properties_typeProperties_username": {
			"type": "string",
			"defaultValue": "root"
		},
		"Onpremsql_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://onpremsqlsathya.vault.azure.net/"
		},
		"ToDatalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://datamigrationsathya.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "datamigration-sathya_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"MySqlonprem_password": {
						"value": "[parameters('MySqlonprem_password')]"
					},
					"Onpremsql_properties_typeProperties_baseUrl": {
						"value": "[parameters('Onpremsql_properties_typeProperties_baseUrl')]"
					},
					"MySqlonprem_properties_typeProperties_server": {
						"value": "[parameters('MySqlonprem_properties_typeProperties_server')]"
					},
					"MySqlonprem_properties_typeProperties_database": {
						"value": "[parameters('MySqlonprem_properties_typeProperties_database')]"
					},
					"MySqlonprem_properties_typeProperties_username": {
						"value": "[parameters('MySqlonprem_properties_typeProperties_username')]"
					},
					"ToDatalake_properties_typeProperties_url": {
						"value": "[parameters('ToDatalake_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}